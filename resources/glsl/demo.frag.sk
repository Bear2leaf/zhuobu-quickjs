in vec3 v_color;
in vec2 v_texcoord;

uniform sampler2D u_texture0;
uniform sampler2D u_texture1;

out vec4 o_color;
void main() {
    o_color = mix(vec4(v_color, 1.0), mix(texture(u_texture0, v_texcoord), texture(u_texture1, v_texcoord), 0.3), 0.9);
    // o_color = texture(u_texture2, v_texcoord);
}